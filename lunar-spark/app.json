{
  "expo": {
    "name": "Lunar Spark",
    "slug": "lunar-spark",
    "version": "1.0.0",
    "scheme": "lunarspark",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#0A0A0A"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.midnight.lunarspark"
    },
    "platforms": [
      "ios"
    ],
    "plugins": [
      "expo-router"
    ]
  }
}
